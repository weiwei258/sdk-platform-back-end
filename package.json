{
  "name": "hackernews-async-ts",
  "version": "1.0.0",
  "description": "hackernews showcase using typescript && egg",
  "private": true,
  "egg": {
    "typescript": true,
    "declarations": true
  },
  "scripts": {
    "start": "egg-scripts start",
    "dev": "egg-bin dev",
    "debug": "egg-bin debug",
    "test-local": "egg-bin test",
    "test": "npm run lint -- --fix && npm run test-local",
    "cov": "egg-bin cov",
    "tsc": "tsc -p tsconfig.json",
    "ci": "npm run lint && npm run cov && npm run tsc",
    "lint": "eslint .",
    "clean": "tsc -b --clean"
  },
  "dependencies": {
    "@eagle-tracker/types": "^2.1.0",
    "@types/co-body": "^6.1.3",
    "@types/jsonwebtoken": "^9.0.2",
    "@types/koa-bodyparser": "^4.3.12",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.0",
    "co-body": "^6.1.0",
    "crypto": "^1.0.1",
    "egg": "^3.11.0",
    "egg-cors": "^2.2.3",
    "egg-multipart": "^3.3.0",
    "egg-redis": "^2.5.0",
    "egg-scripts": "^2.17.0",
    "egg-static": "^2.3.1",
    "egg-ts-typeorm": "^1.1.12",
    "egg-view-nunjucks": "^2.3.0",
    "jsonwebtoken": "^9.0.1",
    "koa-bodyparser": "^4.4.1",
    "moment": "^2.22.0",
    "mysql": "^2.18.1",
    "typedi": "^0.10.0",
    "typeorm": "^0.3.17"
  },
  "devDependencies": {
    "@eggjs/tsconfig": "^1.1.0",
    "@types/cheerio": "^0.22.1",
    "@types/mocha": "^10.0.1",
    "cheerio": "^1.0.0-rc.2",
    "egg-bin": "^5.9.0",
    "egg-mock": "^5.5.0",
    "eslint": "^8.31.0",
    "eslint-config-egg": "^12.1.0",
    "typescript": "^4.9.4"
  },
  "engines": {
    "node": ">=16.0.0"
  }
}
